#include <stdio.h>

int main() {

    int zoneCode = 0 ;
    float weight_kg = 0.0 ;
    float totalCost = 0.0 ;

    if( scanf( "%d %f", &zoneCode, &weight_kg ) != 2 ) {
        return 1 ;
    }//end if

    switch( zoneCode ) {
        case 1:
        if( weight_kg <= 5.0 ) {
            totalCost = 50.0 ;
        }else if ( weight_kg > 5.0 ) {
            totalCost = 80.0 ;
        }//end if
        break ;

        case 2 :
        if( weight_kg <= 10.0 ) {
            totalCost = 150.0 ;
        }else if( weight_kg > 10.0 ){
            totalCost = 250.0 ;
        }//end if
        break ;

        case 3 :
        totalCost = 500.0 ;
        break ;

        default :
        printf( "Invalid Zone Code" ) ;
        break ;

    }//end switch

    if( totalCost > 0.0 || zoneCode <= 3 || zoneCode >= 1 ) {
        printf( "%.2f\n", totalCost ) ;
    }//end if


    return 0 ; 
}//end function
